{
  "containerDefinitions": [
    {
      "name": "ContentDriven",
      "image": "ukpds/content-driven",
      "cpu": 10,
      "memory": 512,
      "essential": true,
      "command": [],
      "portMappings": [
        {
          "hostPort": 82,
          "containerPort": 3000,
          "protocol": "tcp"
        }
      ],
      "logConfiguration": {
        "logDriver": "awslogs",
        "options": {
          "awslogs-group": "ContentDriven",
          "awslogs-region": "eu-west-1"
        }
      }
    }
  ],
  "family": "ContentDriven"
}